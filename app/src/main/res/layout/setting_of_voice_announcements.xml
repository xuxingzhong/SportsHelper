<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.root.sportshelper.SettingOfVoiceAnnouncements"
    android:orientation="vertical">
    <include layout="@layout/back_toolbar"
        android:id="@+id/Setting_toolbar"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_0dp"
        android:layout_weight="@dimen/weight_1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_56dp">
                <TextView
                    android:layout_width="@dimen/dimen_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="@dimen/weight_1"
                    android:text="@string/openVoiceAnnouncements"
                    android:textColor="@color/color_ff191919"
                    android:textSize="@dimen/dimen_16sp"
                    android:gravity="left|center"
                    android:layout_marginLeft="@dimen/dimen_16dp" />
                <ImageView
                    android:id="@+id/openVoiceAnnouncementsSwitch"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/dimen_16dp" />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_30dp"
                android:background="@color/color_d6d6d6"
                android:text="@string/SoundBroadcast"
                android:textColor="@color/color_2387f5"
                android:textSize="@dimen/dimen_12sp"
                android:gravity="left|center"
                android:paddingLeft="@dimen/dimen_16dp"/>
            <LinearLayout
                android:id="@+id/maleVoice"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_56dp">
                <TextView
                    android:layout_width="@dimen/dimen_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="@dimen/weight_1"
                    android:text="@string/maleVoice"
                    android:textColor="@color/color_ff191919"
                    android:textSize="@dimen/dimen_16sp"
                    android:gravity="left|center"
                    android:layout_marginLeft="@dimen/dimen_16dp" />
                <ImageView
                    android:id="@+id/maleVoiceDone"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_done_pre"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/dimen_16dp" />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1dp"
                android:background="@color/color_d6d6d6"
                android:layout_marginLeft="@dimen/dimen_16dp"
                android:layout_marginRight="@dimen/dimen_16dp"/>
            <LinearLayout
                android:id="@+id/femaleVoice"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_56dp">
                <TextView
                    android:layout_width="@dimen/dimen_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="@dimen/weight_1"
                    android:text="@string/femaleVoice"
                    android:textColor="@color/color_ff191919"
                    android:textSize="@dimen/dimen_16sp"
                    android:gravity="left|center"
                    android:layout_marginLeft="@dimen/dimen_16dp" />
                <ImageView
                    android:id="@+id/femaleVoiceDone"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_done_pre"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/dimen_16dp" />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_30dp"
                android:background="@color/color_d6d6d6"
                android:text="@string/BroadcastFrequencyAcDistance"
                android:textColor="@color/color_2387f5"
                android:textSize="@dimen/dimen_12sp"
                android:gravity="left|center"
                android:paddingLeft="@dimen/dimen_16dp"/>
            <LinearLayout
                android:id="@+id/halfkilometers"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_56dp">
                <TextView
                    android:layout_width="@dimen/dimen_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="@dimen/weight_1"
                    android:text="@string/halfkilometers"
                    android:textColor="@color/color_ff191919"
                    android:textSize="@dimen/dimen_16sp"
                    android:gravity="left|center"
                    android:layout_marginLeft="@dimen/dimen_16dp" />
                <ImageView
                    android:id="@+id/halfkilometersDone"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_done_pre"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/dimen_16dp" />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1dp"
                android:background="@color/color_d6d6d6"
                android:layout_marginLeft="@dimen/dimen_16dp"
                android:layout_marginRight="@dimen/dimen_16dp"/>
            <LinearLayout
                android:id="@+id/onekilometers"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_56dp">
                <TextView
                    android:layout_width="@dimen/dimen_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="@dimen/weight_1"
                    android:text="@string/onekilometers"
                    android:textColor="@color/color_ff191919"
                    android:textSize="@dimen/dimen_16sp"
                    android:gravity="left|center"
                    android:layout_marginLeft="@dimen/dimen_16dp" />
                <ImageView
                    android:id="@+id/onekilometersDone"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_done_pre"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/dimen_16dp" />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1dp"
                android:background="@color/color_d6d6d6"
                android:layout_marginLeft="@dimen/dimen_16dp"
                android:layout_marginRight="@dimen/dimen_16dp"/>
            <LinearLayout
                android:id="@+id/twokilometers"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_56dp">
                <TextView
                    android:layout_width="@dimen/dimen_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="@dimen/weight_1"
                    android:text="@string/twokilometers"
                    android:textColor="@color/color_ff191919"
                    android:textSize="@dimen/dimen_16sp"
                    android:gravity="left|center"
                    android:layout_marginLeft="@dimen/dimen_16dp" />
                <ImageView
                    android:id="@+id/twokilometersDone"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_done_pre"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/dimen_16dp" />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1dp"
                android:background="@color/color_d6d6d6"
                android:layout_marginLeft="@dimen/dimen_16dp"
                android:layout_marginRight="@dimen/dimen_16dp"/>
            <LinearLayout
                android:id="@+id/threekilometers"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_56dp">
                <TextView
                    android:layout_width="@dimen/dimen_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="@dimen/weight_1"
                    android:text="@string/threekilometers"
                    android:textColor="@color/color_ff191919"
                    android:textSize="@dimen/dimen_16sp"
                    android:gravity="left|center"
                    android:layout_marginLeft="@dimen/dimen_16dp" />
                <ImageView
                    android:id="@+id/threekilometersDone"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_done_pre"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/dimen_16dp" />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_30dp"
                android:background="@color/color_d6d6d6"
                android:text="@string/BroadcastFrequencyAcTime"
                android:textColor="@color/color_2387f5"
                android:textSize="@dimen/dimen_12sp"
                android:gravity="left|center"
                android:paddingLeft="@dimen/dimen_16dp"/>
            <LinearLayout
                android:id="@+id/Fiveminutes"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_56dp">
                <TextView
                    android:layout_width="@dimen/dimen_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="@dimen/weight_1"
                    android:text="@string/Fiveminutes"
                    android:textColor="@color/color_ff191919"
                    android:textSize="@dimen/dimen_16sp"
                    android:gravity="left|center"
                    android:layout_marginLeft="@dimen/dimen_16dp" />
                <ImageView
                    android:id="@+id/FiveminutesDone"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_done_pre"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/dimen_16dp" />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1dp"
                android:background="@color/color_d6d6d6"
                android:layout_marginLeft="@dimen/dimen_16dp"
                android:layout_marginRight="@dimen/dimen_16dp"/>
            <LinearLayout
                android:id="@+id/tenminutes"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_56dp">
                <TextView
                    android:layout_width="@dimen/dimen_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="@dimen/weight_1"
                    android:text="@string/tenminutes"
                    android:textColor="@color/color_ff191919"
                    android:textSize="@dimen/dimen_16sp"
                    android:gravity="left|center"
                    android:layout_marginLeft="@dimen/dimen_16dp" />
                <ImageView
                    android:id="@+id/tenminutesDone"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_done_pre"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/dimen_16dp" />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1dp"
                android:background="@color/color_d6d6d6"
                android:layout_marginLeft="@dimen/dimen_16dp"
                android:layout_marginRight="@dimen/dimen_16dp"/>
            <LinearLayout
                android:id="@+id/Twentyminutes"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_56dp">
                <TextView
                    android:layout_width="@dimen/dimen_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="@dimen/weight_1"
                    android:text="@string/Twentyminutes"
                    android:textColor="@color/color_ff191919"
                    android:textSize="@dimen/dimen_16sp"
                    android:gravity="left|center"
                    android:layout_marginLeft="@dimen/dimen_16dp" />
                <ImageView
                    android:id="@+id/TwentyminutesDone"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_done_pre"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/dimen_16dp" />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1dp"
                android:background="@color/color_d6d6d6"
                android:layout_marginLeft="@dimen/dimen_16dp"
                android:layout_marginRight="@dimen/dimen_16dp"/>
            <LinearLayout
                android:id="@+id/Thirtyminutes"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_56dp">
                <TextView
                    android:layout_width="@dimen/dimen_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="@dimen/weight_1"
                    android:text="@string/Thirtyminutes"
                    android:textColor="@color/color_ff191919"
                    android:textSize="@dimen/dimen_16sp"
                    android:gravity="left|center"
                    android:layout_marginLeft="@dimen/dimen_16dp" />
                <ImageView
                    android:id="@+id/ThirtyminutesDone"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_done_pre"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/dimen_16dp" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
